<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.crudjavafx.NewCharacterController">
    <children>
        <Label layoutX="27.0" layoutY="14.0" text="Nuevo personaje">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label layoutX="27.0" layoutY="49.0" text="Nombre">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <TextField fx:id="txtNombre" layoutX="27.0" layoutY="69.0" prefHeight="25.0" prefWidth="546.0" />
        <Label layoutX="27.0" layoutY="100.0" text="Clase">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <ComboBox fx:id="comboClase" layoutX="27.0" layoutY="120.0" prefHeight="30.0" prefWidth="546.0" />
        <Label layoutX="32.0" layoutY="150.0" text="Dinero">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <TextField fx:id="txtDinero" layoutX="27.0" layoutY="170.0" prefHeight="25.0" prefWidth="546.0" />
        <Button fx:id="btnCreate" layoutX="269.0" layoutY="331.0" mnemonicParsing="false" onAction="#create" prefHeight="38.0" prefWidth="95.0" text="Crear">
            <font>
                <Font size="16.0" />
            </font>
        </Button>
        <Button fx:id="btnCancel" layoutX="27.0" layoutY="331.0" mnemonicParsing="false" onAction="#cancel" prefHeight="38.0" prefWidth="95.0" text="Cancelar">
            <font>
                <Font size="16.0" />
            </font>
        </Button>
        <Button fx:id="btnShowClasses" layoutX="27.0" layoutY="270.0" mnemonicParsing="false" onAction="#showClasses" prefHeight="38.0" prefWidth="95.0" text="Clases">
            <font>
                <Font size="16.0" />
            </font>
        </Button>

    </children>

</AnchorPane>
